{
  "name": "Angular UI Project",
  "language": "typescript",
  "exclude": [
    "src/main/java/**/*",
    "src/test/java/**/*",
    "target/**/*",
    "*.java",
    "mvnw*",
    "*.xml",
    ".mvn/**/*",
    "pom.xml",
    ".classpath",
    ".project",
    ".settings/**/*",
    "gradle/**/*",
    "gradlew*",
    "build.gradle",
    ".gradle/**/*",
    "bin/**/*",
    "build/**/*"
  ],
  "include": [
    "src/app/**/*",
    "src/assets/**/*",
    "src/environments/**/*",
    "angular.json",
    "package.json",
    "tsconfig*.json",
    "*.ts",
    "*.js",
    "*.html",
    "*.scss",
    "*.css"
  ],
  "root": "src/main/webapp"
}
